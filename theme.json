	"styles": {
		"color": {
			"background": "var:preset|color|base",
			"text": "var:preset|color|contrast"
		},
		"spacing": {
			"blockGap": "1.2rem",
			"padding": {
				"left": "var:preset|spacing|50",
				"right": "var:preset|spacing|50"
			}
		},
		"typography": {
			"fontFamily": "var:preset|font-family|fira-sans",
			"fontSize": "var:preset|font-size|medium",
			"letterSpacing": "-0.01em",
			"lineHeight": "1.5"
		},
			"blocks": {
				"core/avatar": {
					"border": {
						"radius": "100px"
					},
					"filter": {
						"duotone": "var:preset|duotone|midnight-filter"
					}
				},
				"core/button": {
					"border": {
						"radius": "0px"
					},
					"color": {
						"background": "var:preset|color|contrast",
						"text": "var:preset|color|base"
					},
					":hover": {
						"color": {
							"background": "color-mix(in srgb, var(--wp--preset--color--contrast) 85%, transparent)",
							"text": "var:preset|color|base"
						}
					},
					"typography": {
						"fontWeight": "600",
						"textTransform": "none"
					},
					"variations": {
						"outline": {
							"border": {
								"color": "currentColor",
								"width": "1px"
							},
							"css": ".wp-block-button__link:not(.has-background):hover {background-color:color-mix(in srgb, var(--wp--preset--color--contrast) 10%, transparent);}",
							"spacing": {
								"padding": {
									"bottom": "calc(1rem - 1px)",
									"left": "calc(2.25rem - 1px)",
									"right": "calc(2.25rem - 1px)",
									"top": "calc(1rem - 1px)"
								}
							}
						}
					}
				},
			"core/columns": {
				"spacing": {
					"blockGap": "var:preset|spacing|50"
				}
			},
			"core/buttons": {
				"spacing": {
					"blockGap": "var:preset|spacing|40"
				}
			},
				"core/code": {
					"typography": {
						"fontFamily": "var:preset|font-family|jetbrains-mono",
						"fontSize": "var:preset|font-size|small",
						"fontWeight": "400"
					},
					"color": {
						"background": "var:preset|color|accent-2",
						"text": "var:preset|color|contrast"
					},
					"spacing": {
						"padding": {
							"right": "var:preset|spacing|40",
							"bottom": "var:preset|spacing|40",
							"left": "var:preset|spacing|40",
							"top": "var:preset|spacing|40"
						}
					}
				},
				"core/cover": {
					"filter": {
						"duotone": "var:preset|duotone|midnight-filter"
					}
				},
				"core/image": {
					"filter": {
						"duotone": "var:preset|duotone|midnight-filter"
					}
				},
				"core/post-featured-image": {
					"filter": {
						"duotone": "var:preset|duotone|midnight-filter"
					}
				},
				"core/site-logo": {
					"filter": {
						"duotone": "var:preset|duotone|midnight-filter"
					}
				},
			"core/comment-author-name": {
				"color": {
					"text": "var:preset|color|accent-4"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var:preset|color|accent-4"
						},
						"typography": {
							"textDecoration": "none"
						},
						":hover": {
